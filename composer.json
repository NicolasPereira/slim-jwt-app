{
    "require": {
        "doctrine/annotations": "^1.13",
        "doctrine/migrations": "^3.5",
        "doctrine/orm": "*",
        "php-di/php-di": "^6.4",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "symfony/cache": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
